L10:		; REM FOR LOOP
L20:		PushInt 1
		PullVar I
		PushInt 5
		PullVar IEND
		PushInt 1
		PullVar ISTEP
LOOP0:
		PushVar IEND
		PushVar I
		jsr LT
		jsr PULL
		lda r0L
		beq LOOP0CONT
		jmp LOOP0END
LOOP0CONT:
L23:		PushInt 1
		PullVar J
		PushInt 3
		PullVar JEND
		PushInt 1
		PullVar JSTEP
LOOP1:
		PushVar JEND
		PushVar J
		jsr LT
		jsr PULL
		lda r0L
		beq LOOP1CONT
		jmp LOOP1END
LOOP1CONT:
L25:		LoadAddress S0		; to r0
